@model IEnumerable<PeriodAid.Models.Off_Manager_Task>

<div class="weui_cells_title">当日签到数 (@Html.Encode(Model.Count()))</div>
<div class="weui_cells weui_cells_access">
    @foreach (var item in Model)
    {
        <a class="weui_cell" href="/Seller/Wx_Senior_CheckInDetails/@item.Id">
            <div class="weui_cell_bd weui_cell_primary">
                <p>@Html.Encode(item.NickName)</p>
            </div>
            <div class="weui_cell_ft">@Html.Encode(item.Off_Manager_CheckIn.Count(m=>m.Canceled==false))</div>
        </a>
    }
</div>

