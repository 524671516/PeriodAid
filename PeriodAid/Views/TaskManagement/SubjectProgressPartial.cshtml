@using PeriodAid.Models;
@model PeriodAid.Models.Subject
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <h4 class="modal-title">今日进度</h4>
</div>
<div class="container-fluid">
    <div class="row modal-nav-panel">
        <div class="col-md-3 tm_modal_ul_wrap">
            <ul class="list-group tm_modal-nav tm_modal_nav_ul">
                <li class="tm-show-view-1 list-group-item tm_active" data-href="/TaskManagement/SubjectFinishProgressPartial">已完成</li>
                <li class="tm-show-view-1 list-group-item" data-href="/TaskManagement/SubjectUntreatedProgressPartial">待处理</li>
            </ul>
        </div>
        <div class="col-md-9 tm_modal_ul_wrap">
            @*<p class="text-center" style="margin-top:10px;" id="loading-p"><i class="fa fa-spinner fa-pulse fa-3x fa-fw margin-bottom color-gray"></i></p>*@
            <ul class="list-group tm_modal-list tm_modal_list_ul" id="tm_modal_list_area"></ul>
        </div>
    </div>
</div>
<div class="modal-footer" style="border:none;">
    <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
</div>
