
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no, minimal-ui">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <title>管理页面</title>
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome-ie7.min.css" rel="stylesheet" />
    <link href="~/Content/framework7.ios.min.css" rel="stylesheet" />
    <link href="~/Content/framework7.ios.colors.min.css" rel="stylesheet" />
    <link href="~/Content/my-app.css" rel="stylesheet" />
</head>
<body>
    <div class="notifications list-block media-list">
        <ul>
            <li class="notification-item">
            </li>
        </ul>
    </div>
    <div class="statusbar-overlay"></div>
    <!-- Panels overlay-->
    <div class="panel-overlay"></div>
    <!-- Left panel with reveal effect-->
    <div class="panel panel-left panel-reveal left-nav" id="left-user-panel">
        
    </div>
    <!-- Views -->
    <div class="views">
        <div class="view view-main" data-page="Home">
            <!-- Top Navbar-->
            <div class="navbar">
                <div class="navbar-inner">
                    <div class="center sliding"><strong>寿全斋</strong></div>
                    <div class="right">
                        <a href="" class="link icon-only open-panel"><i class="icon icon-bars"></i></a>
                    </div>
                </div>
            </div>
            <!-- Pages container, because we use fixed-through navbar and toolbar, it has additional appropriate classes-->
            <div class="pages navbar-through toolbar-through">
                <!-- Page, "data-page" contains page name -->
                <div data-page="Home" class="page">
                    <!-- Scrollable page content -->
                    <div class="page-content pull-to-refresh-content" data-ptr-distance="50">
                        <div class="pull-to-refresh-layer">
                            <div class="preloader"></div>
                            <div class="pull-to-refresh-arrow"></div>
                        </div>
                        <div class="list-block">
                            <div class="list-group">
                                <ul>
                                    <li class="list-block media-list">
                                        <div class="item-content">
                                            <div class="item-media img-box color-blue">
                                                <i class="fa fa-calendar-plus-o" aria-hidden="true"></i>
                                            </div>
                                            <div class="item-inner">
                                                <div class="item-title"><strong>本月工作汇报</strong></div>
                                                <div class="item-text">日期：2016-07-05</div>
                                                <div class="item-title-row">
                                                    <div class="item-title">本月工作日：<span>22</span></div>
                                                    <div class="item-after">已完成：<span class="num">10</span></div>
                                                </div>
                                            </div>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                            <div class="list-group">
                                <ul>
                                    <li class="list-group-title">工作汇报</li>
                                    <li>
                                        <a href="/SellerTask/CreateSellerReport?id=342" class="item-link item-content">
                                            <div class="item-inner"><div class="item-title">每日任务</div></div>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <div class="list-group">
                                <ul>
                                    <li class="list-group-title">其他功能</li>
                                    <li>
                                        <a href="/SellerTask/SellerTaskList?id=342" class="item-link item-content">
                                            <div class="item-inner"><div class="item-title">查看历史</div></div>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="/SellerTask/UpdateAccountInfo?id=342" class="item-link item-content">
                                            <div class="item-inner">
                                                <div class="item-title">完善信息</div>
                                                <div class="item-after"><span class="badge bg-red hidden"></span></div>
                                            </div>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @{
        Html.RenderAction("WxJSAppPartial");
    }
    <script src="~/Scripts/framework7.min.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.form.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="http://res.wx.qq.com/open/js/jweixin-1.0.0.js" type="text/javascript"></script>
    <script src="~/Scripts/my-validate.js"></script>
    <script src="~/Scripts/my-app.js" charset="utf-8"></script>
    <script src="~/Scripts/maapp.js"></script>
</body>
</html>