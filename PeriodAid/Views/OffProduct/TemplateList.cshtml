
@{
    ViewBag.Title = "Off_Template_List";
    Layout = "~/Views/Shared/_OffLayout.cshtml";
    ViewBag.Current = "headingEight";
}
<div class="container">
    <ul class="list-inline list-unstyled row">
        <li><a class="btn btn-info btn-sm" href="/OffProduct/CreateTemplate"><i class="fa fa-plus"></i>&nbsp;&nbsp;添加模板</a></li>
    </ul>
</div>
<div id="offproduct-template-list">

</div>
<script>
    $(function () {
        $.ajax({
            url: "/OffProduct/TemplateListPartial",
            success: function (data) {
                $("#offproduct-template-list").html(data)
            }
        })
    });
</script>