@{
    ViewBag.Title = "上传销售员信息";
    Layout = "~/Views/Shared/_OffLayout.cshtml";
}
@Html.Partial("Off_Seller_Controller")
<br />
<h4><b>上传销售员信息</b></h4>
<div class="row">
    @using (Html.BeginForm("UploadSeller", "OfflineSales", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <div class="col-xs-4">
            <div class="input-group">
                <input type="file" name="xlsxfile" id="xlsxfile" class="form-control input-sm" accept=".xlsx" />
                <span class="input-group-btn">
                    <button class="btn btn-success btn-sm">上传</button>
                </span>
            </div>
        </div>
        <div class="col-xs-8">
            <a class="btn btn-info btn-sm" href="~/Content/xlsx/联系人信息表-Template.xlsx">下载模板</a>
        </div>
    }
</div>