
@{
    ViewBag.Title = "Off_Manager_Request_List";
    Layout = "~/Views/Shared/_OffLayout.cshtml";
    ViewBag.Current = "headingSix";
}
<div id="requestList">

</div>
<script>
    $(function () {
        var url = "/OfflineSales/Off_Manager_Request_List_Ajax"
        $.ajax({
            url: url,
            success: function (list) {
                $("#requestList").html(list)
            }
        })
    })
</script>

