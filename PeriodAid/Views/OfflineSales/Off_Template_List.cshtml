
@{
    ViewBag.Title = "Off_Template_List";
    Layout = "~/Views/Shared/_OffLayout.cshtml";
}
@section page{
    8
    }
<div class="row">
    <div class="col-xs-8">
        <a class="btn btn-info btn-sm" href="/OfflineSales/Off_Template_Create"><i class="fa fa-plus"></i>&nbsp;&nbsp;添加模板</a>
    </div>
</div>
<div id="templateList">

</div>
<script>
    $(function () {
        var link_url = "/OfflineSales/Off_Template_List_Ajax"
        $.ajax({
            url: link_url,
            success: function (data) {
                $("#templateList").html(data)
            }
        })
    })
</script>